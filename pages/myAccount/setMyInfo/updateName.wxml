<view class="page">
  <view class="flash weui-cells__title">
    <text class="tipsText" decode='true'>{{tipsContent}}</text>
  </view>
  <view class="weui-cells no-border weui-cells_after-title">
    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__bd">
        <input class="weui-input input-text" value="{{nickName}}" />
      </view>
    </view>
  </view>
  <view class="btn-wrap">
    <button class="flash-btn" size="mini" hover-class="other-button-hover">完成</button>
    <view class="flash-btn-mask"></view>
  </view>
</view>